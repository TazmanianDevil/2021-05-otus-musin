<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="fileParser" class="ru.otus.homework.service.impl.CsvFileQuestionsParser">
        <constructor-arg name="resourceFile" value="data/questions.csv"/>
    </bean>

    <bean id="questionsPrinter" class="ru.otus.homework.service.impl.ConsoleQuestionsPrinter">
        <constructor-arg name="parser" ref="fileParser"/>
    </bean>
</beans>
